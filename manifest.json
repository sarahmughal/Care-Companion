{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "4",
  "author": "Jennifer John",
  //"background": {
  //"scripts": ["background.js"]
  //},
  "browser_action":{
    "default_icon": "smiley.png"
  },
  "content_scripts": [
  {
    "matches": [
      "https://*.google.com/*"
    ],
    "js": ["content.js"]
  }
    chrome.experimental.cookies.getAll(object details, function callback)
]
}

function updateUserLoginStatusUi() {
  chrome.experimental.cookies.get({
    url: "https://www.google.com",
    name: "COOKIE"
  }, function(cookie) {
    if (cookie) {
      displayLoggedInUi();
    } else {
      displayLoggedOutUi();
    }
  });
}
